#include<stdio.h>
int max_of_four(int,int,int,int);

int main(){
	int a,b,c,d,ans;
	scanf("%d %d %d %d",&a,&b,&c,&d);
	ans=max_of_four(a,b,c,d);
	printf("Maximum of four will be %d",ans);
}
int max_of_four(int a,int b,int c,int d){
	int max;
	max=a;
	if(b>max)
		max=b;
	if(c>max)
		max=c;
	if(d>max)
		max=d;
	return max;
}
